ax.helpers.axset.af()
{
    local -a PIPE_STATUS=( "$PIPESTATUS" )
    
    local -i RV=$?
    local SMS="$-"
    local SDS="$$"
    local -i SNV=$[ $# -1 ]
    local SUS=${@: -1:1}
    
    case $1 in
    
    _axhelpercall)
        shift
        while true; do
            _hrule "\e[31;1m‚ñÅNormal‚ñÅEnvironment‚ñÅVariables‚ñÅ$(_hrule "‚ñÅ")"        
            set | sed 's//[7mESC[0m/g'
            _hrule "\e[31;1m‚ñÅExtra‚ñÅEnvironment‚ñÅVariables‚ñÅ$(_hrule "‚ñÅ")"        
            local -A POSITIONAL_PARAMETERS=( "$@" )
            declare -p  POSITIONAL_PARAMETERS | sed
            echo 
        done
        ;;
    *)
        echo "fatal: this must be called by the ax.var.set or other alias attached to it, do not alias directly to this func!"
        return 127
        ;;         
    esac
    
}
