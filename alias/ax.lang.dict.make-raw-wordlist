alias ax.lang.dict.make-raw-wordlist='SECONDS=0;echo "timer-reset... making $LANG raw wordlist..."; mkdir --parents "$AX_BASE/cache/dict/$LANG" --parents; ln -fsT "$AX_BASE/cache/dict/$LANG" "$AX_BASE/cache/dict/$LANGUAGE"; grep -P "^[a-z]{4,}\$" /usr/share/dict/words > /$AX_BASE/cache/dict/$LANG/words; echo "complete ($SECONDS/RET=$?/SIZE=$(stat $AX_BASE/cache/dict/$LANG/words --format %s))"'
