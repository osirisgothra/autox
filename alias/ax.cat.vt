alias ax.cat.vt='REPLY=00;clear;echo "remote style entry, NO BACKSPACE ALLOWED! enter TWO DIGIT numbers or ** [qq] to quit **";echo "This Message Will Disappear After The First Run!"; echo "MULTIPLE duplicate lines will NOT be shown!!"; sleep 1;while true; do read -p $REPLY"[2D" -i $REPLY -n2; sudo clear; [[ $REPLY == qq ]] && break; echo "[0;1;44m Dump of Screen $REPLY [K [0m";sudo screendump $REPLY | uniq; done'
